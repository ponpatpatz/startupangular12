{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "admApp": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/adm-app",
      "sourceRoot": "projects/adm-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/admApp",
            "index": "projects/adm-app/src/index.html",
            "main": "projects/adm-app/src/main.ts",
            "polyfills": "projects/adm-app/src/polyfills.ts",
            "tsConfig": "projects/adm-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/adm-app/src/favicon.ico",
              "projects/adm-app/src/assets",
              "projects/adm-app/src/manifest.webmanifest"
            ],
            "styles": [
              "./node_modules/primeicons/primeicons.css",
              "./node_modules/primeng/resources/themes/bootstrap4-light-purple/theme.css",
              "./node_modules/primeng/resources/primeng.min.css",
              "./node_modules/font-awesome/css/font-awesome.min.css",
              "./node_modules/animate.css/animate.min.css",
              "./node_modules/sweetalert2/dist/sweetalert2.min.css",
              "./node_modules/quill/dist/quill.core.css",
              "./node_modules/quill/dist/quill.snow.css",
              "./node_modules/fullcalendar/dist/fullcalendar.min.css",
              "./node_modules/leaflet/dist/leaflet.css",
              "projects/adm-app/src/styles.scss"
            ],
            "scripts": [
              "./node_modules/sweetalert2/dist/sweetalert2.min.js",
              "./node_modules/quill/dist/quill.js"
            ],
            "serviceWorker": true,
            "ngswConfigPath": "projects/adm-app/ngsw-config.json"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/adm-app/src/environments/environment.ts",
                  "with": "projects/adm-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "admApp:build:production"
            },
            "development": {
              "browserTarget": "admApp:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "admApp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/adm-app/src/test.ts",
            "polyfills": "projects/adm-app/src/polyfills.ts",
            "tsConfig": "projects/adm-app/tsconfig.spec.json",
            "karmaConfig": "projects/adm-app/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/adm-app/src/favicon.ico",
              "projects/adm-app/src/assets",
              "projects/adm-app/src/manifest.webmanifest"
            ],
            "styles": [
              "projects/adm-app/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "admApp"
}
